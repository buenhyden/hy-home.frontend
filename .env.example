# Environment Variables Example
# Copy this file to .env and modify the values

# API URL
VITE_API_URL=http://localhost:3000/api

# App Environment
VITE_APP_ENV=development

# Docker Configuration
# Image tag (latest for production, dev for development)
IMAGE_TAG=latest
# Container registry
REGISTRY=ghcr.io
# Image name
IMAGE_NAME=buenhyden/hy-home.frontend
# Port mapping
PORT=3000
# Node environment
NODE_ENV=production
